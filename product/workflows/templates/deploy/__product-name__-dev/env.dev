# @saflib/env
ALLOW_DB_CREATION=true
DEPLOYMENT_NAME=development
MOCK_INTEGRATIONS=true
NODE_ENV=development
PROTOCOL=http
TZ=UTC

# __product-name__-specific @saflib/env
__PRODUCT_NAME___TRADEMARK=__ProductName__
__PRODUCT_NAME___DOMAIN=__product-name__.docker.localhost
__PRODUCT_NAME___CLIENT_SUBDOMAINS=,auth,app,admin,grafana,account
__PRODUCT_NAME___SERVICE_SUBDOMAINS=identity,__product-name__
__PRODUCT_NAME___POSTHOG_PROJECT_API_KEY=

# @saflib/identity-*
__PRODUCT_NAME___IDENTITY_SERVICE_HTTP_HOST=__product-name__-monolith:3000
__PRODUCT_NAME___IDENTITY_SERVICE_GRPC_HOST=__product-name__-monolith:50051
__PRODUCT_NAME___IDENTITY_SERVICE_ADMIN_EMAILS=admin@__organization-name__.com
__PRODUCT_NAME___IDENTITY_SERVICE_DISABLE_RATE_LIMITING=true
__PRODUCT_NAME___SENTRY_DSN=

# @saflib/email
NODEMAILER_TRANSPORT_CONFIG=

# template-package-http
__PRODUCT_NAME___SERVICE_HTTP_HOST=__product-name__-monolith:3001

# Integrations
AZURE_CLIENT_ID=testkeyfordev
AZURE_HOT_BLOB_STORAGE_URL=http://azurite:10000/devstoreaccount1/
